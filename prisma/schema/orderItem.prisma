model orderItem {
    id          String @id @default(ulid())
    order_id    String
    order       Order  @relation(fields: [order_id], references: [id], onDelete: Cascade)
    meal_id     String
    meal        Meal   @relation(fields: [meal_id], references: [id])
    quantity    Int    @default(1)
    price       Float
    total_price Float

    @@map("order-items")
}
